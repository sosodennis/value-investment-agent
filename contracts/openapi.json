{
  "components": {
    "schemas": {
      "AgentOutputArtifact": {
        "description": "Standard container for all Sub-Agent outputs.\n\nThis structure implements a three-tier data architecture:\n- L1 (summary): Message bubble text (<500 chars) - Always present\n- L2 (preview): Hot data for immediate UI rendering (<1KB) - Optional\n- L3 (reference): Cold data pointer for async loading - Optional\n\nThis ensures efficient state management by avoiding large data duplication\nin LangGraph checkpoints.",
        "properties": {
          "preview": {
            "anyOf": [
              {
                "additionalProperties": true,
                "type": "object"
              },
              {
                "type": "null"
              }
            ],
            "description": "L2: Hot data for immediate UI rendering (<1KB). Generated by Mapper from State.",
            "title": "Preview"
          },
          "reference": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/ArtifactReference"
              },
              {
                "type": "null"
              }
            ],
            "description": "L3: Cold data pointer for async loading. Points to Artifact Store."
          },
          "summary": {
            "description": "L1: Short summary for message bubble (<500 chars)",
            "title": "Summary",
            "type": "string"
          }
        },
        "required": [
          "summary"
        ],
        "title": "AgentOutputArtifact",
        "type": "object"
      },
      "AgentStatusesResponse": {
        "properties": {
          "agent_outputs": {
            "additionalProperties": {
              "$ref": "#/components/schemas/AgentOutputArtifact"
            },
            "title": "Agent Outputs",
            "type": "object"
          },
          "current_node": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Current Node"
          },
          "node_statuses": {
            "additionalProperties": {
              "type": "string"
            },
            "title": "Node Statuses",
            "type": "object"
          }
        },
        "required": [
          "node_statuses",
          "agent_outputs"
        ],
        "title": "AgentStatusesResponse",
        "type": "object"
      },
      "ArtifactReference": {
        "description": "L3 冷數據指針 (Cold Data Pointer)\n\nPoints to large artifacts stored in the Artifact Store.\nFrontend can fetch these asynchronously via the download_url.",
        "properties": {
          "artifact_id": {
            "description": "UUID of the artifact in the store",
            "title": "Artifact Id",
            "type": "string"
          },
          "download_url": {
            "description": "API endpoint to fetch the artifact (e.g., /api/artifacts/{id})",
            "title": "Download Url",
            "type": "string"
          },
          "type": {
            "description": "Artifact type identifier (e.g., 'financial_report', 'news_analysis')",
            "title": "Type",
            "type": "string"
          }
        },
        "required": [
          "artifact_id",
          "download_url",
          "type"
        ],
        "title": "ArtifactReference",
        "type": "object"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "title": "Detail",
            "type": "array"
          }
        },
        "title": "HTTPValidationError",
        "type": "object"
      },
      "HumanTickerSelection": {
        "description": "Payload for ticker selection interrupt.",
        "properties": {
          "candidates": {
            "items": {
              "$ref": "#/components/schemas/TickerCandidate"
            },
            "title": "Candidates",
            "type": "array"
          },
          "intent": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/IntentExtraction"
              },
              {
                "type": "null"
              }
            ]
          },
          "reason": {
            "default": "Multiple tickers found or ambiguity detected.",
            "title": "Reason",
            "type": "string"
          },
          "type": {
            "const": "ticker_selection",
            "default": "ticker_selection",
            "title": "Type",
            "type": "string"
          }
        },
        "title": "HumanTickerSelection",
        "type": "object"
      },
      "IntentExtraction": {
        "description": "Extracted intent from user query.",
        "properties": {
          "company_name": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "The name of the company or ticker mentioned by the user.",
            "title": "Company Name"
          },
          "is_valuation_request": {
            "default": true,
            "description": "Whether the user is asking for a financial valuation (default: True).",
            "title": "Is Valuation Request",
            "type": "boolean"
          },
          "reasoning": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Brief reasoning for the extraction.",
            "title": "Reasoning"
          },
          "ticker": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "The stock ticker if explicitly mentioned.",
            "title": "Ticker"
          }
        },
        "title": "IntentExtraction",
        "type": "object"
      },
      "InterruptResumePayload": {
        "properties": {
          "selected_symbol": {
            "title": "Selected Symbol",
            "type": "string"
          },
          "ticker": {
            "title": "Ticker",
            "type": "string"
          }
        },
        "title": "InterruptResumePayload",
        "type": "object"
      },
      "MessageResponse": {
        "properties": {
          "content": {
            "title": "Content",
            "type": "string"
          },
          "created_at": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Created At"
          },
          "data": {
            "anyOf": [
              {
                "additionalProperties": true,
                "type": "object"
              },
              {
                "items": {},
                "type": "array"
              },
              {
                "type": "string"
              },
              {
                "type": "integer"
              },
              {
                "type": "number"
              },
              {
                "type": "boolean"
              },
              {
                "type": "null"
              }
            ],
            "title": "Data"
          },
          "id": {
            "title": "Id",
            "type": "string"
          },
          "role": {
            "enum": [
              "user",
              "assistant",
              "system",
              "tool"
            ],
            "title": "Role",
            "type": "string"
          },
          "type": {
            "default": "text",
            "title": "Type",
            "type": "string"
          }
        },
        "required": [
          "id",
          "role",
          "content"
        ],
        "title": "MessageResponse",
        "type": "object"
      },
      "RequestSchema": {
        "properties": {
          "message": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Message"
          },
          "resume_payload": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/InterruptResumePayload"
              },
              {
                "type": "null"
              }
            ]
          },
          "thread_id": {
            "title": "Thread Id",
            "type": "string"
          }
        },
        "required": [
          "thread_id"
        ],
        "title": "RequestSchema",
        "type": "object"
      },
      "StreamStartResponse": {
        "properties": {
          "status": {
            "enum": [
              "started",
              "running"
            ],
            "title": "Status",
            "type": "string"
          },
          "thread_id": {
            "title": "Thread Id",
            "type": "string"
          }
        },
        "required": [
          "status",
          "thread_id"
        ],
        "title": "StreamStartResponse",
        "type": "object"
      },
      "ThreadStateResponse": {
        "properties": {
          "agent_outputs": {
            "additionalProperties": {
              "$ref": "#/components/schemas/AgentOutputArtifact"
            },
            "title": "Agent Outputs",
            "type": "object"
          },
          "interrupts": {
            "items": {
              "$ref": "#/components/schemas/HumanTickerSelection"
            },
            "title": "Interrupts",
            "type": "array"
          },
          "is_running": {
            "title": "Is Running",
            "type": "boolean"
          },
          "last_seq_id": {
            "title": "Last Seq Id",
            "type": "integer"
          },
          "messages": {
            "items": {
              "$ref": "#/components/schemas/MessageResponse"
            },
            "title": "Messages",
            "type": "array"
          },
          "next": {
            "anyOf": [
              {
                "items": {
                  "type": "string"
                },
                "type": "array"
              },
              {
                "type": "null"
              }
            ],
            "title": "Next"
          },
          "node_statuses": {
            "additionalProperties": {
              "type": "string"
            },
            "title": "Node Statuses",
            "type": "object"
          },
          "resolved_ticker": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Resolved Ticker"
          },
          "status": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Status"
          },
          "thread_id": {
            "title": "Thread Id",
            "type": "string"
          }
        },
        "required": [
          "thread_id",
          "messages",
          "interrupts",
          "is_running",
          "node_statuses",
          "agent_outputs",
          "last_seq_id"
        ],
        "title": "ThreadStateResponse",
        "type": "object"
      },
      "TickerCandidate": {
        "description": "A potential ticker match from search.",
        "properties": {
          "confidence": {
            "default": 1.0,
            "description": "Match confidence score (0-1)",
            "title": "Confidence",
            "type": "number"
          },
          "exchange": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Exchange code",
            "title": "Exchange"
          },
          "name": {
            "description": "Company name",
            "title": "Name",
            "type": "string"
          },
          "symbol": {
            "description": "Stock ticker symbol",
            "title": "Symbol",
            "type": "string"
          },
          "type": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Security type (stock, ETF, etc.)",
            "title": "Type"
          }
        },
        "required": [
          "symbol",
          "name"
        ],
        "title": "TickerCandidate",
        "type": "object"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "title": "Location",
            "type": "array"
          },
          "msg": {
            "title": "Message",
            "type": "string"
          },
          "type": {
            "title": "Error Type",
            "type": "string"
          }
        },
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError",
        "type": "object"
      }
    }
  },
  "info": {
    "description": "Pure FastAPI implementation for FinGraph Valuation Agent (Path B)",
    "title": "Neuro-Symbolic Valuation Engine API",
    "version": "2.0"
  },
  "openapi": "3.1.0",
  "paths": {
    "/api/artifacts/{artifact_id}": {
      "get": {
        "description": "Retrieve artifact data with HTTP caching.",
        "operationId": "get_artifact_api_artifacts__artifact_id__get",
        "parameters": [
          {
            "in": "path",
            "name": "artifact_id",
            "required": true,
            "schema": {
              "title": "Artifact Id",
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {}
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "Get Artifact"
      }
    },
    "/history/{thread_id}": {
      "get": {
        "description": "Retrieve persistent history with cursor pagination.",
        "operationId": "get_history_history__thread_id__get",
        "parameters": [
          {
            "in": "path",
            "name": "thread_id",
            "required": true,
            "schema": {
              "title": "Thread Id",
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "limit",
            "required": false,
            "schema": {
              "default": 20,
              "title": "Limit",
              "type": "integer"
            }
          },
          {
            "in": "query",
            "name": "before",
            "required": false,
            "schema": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "title": "Before"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/MessageResponse"
                  },
                  "title": "Response Get History History  Thread Id  Get",
                  "type": "array"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "Get History"
      }
    },
    "/stream": {
      "post": {
        "description": "Start or resume a job.",
        "operationId": "stream_agent_stream_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RequestSchema"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/StreamStartResponse"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "Stream Agent"
      }
    },
    "/stream/{thread_id}": {
      "get": {
        "description": "Attach to an existing background job's event stream.",
        "operationId": "attach_stream_stream__thread_id__get",
        "parameters": [
          {
            "in": "path",
            "name": "thread_id",
            "required": true,
            "schema": {
              "title": "Thread Id",
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {}
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "Attach Stream"
      }
    },
    "/thread/{thread_id}": {
      "get": {
        "description": "Retrieve history and job status.",
        "operationId": "get_thread_history_thread__thread_id__get",
        "parameters": [
          {
            "in": "path",
            "name": "thread_id",
            "required": true,
            "schema": {
              "title": "Thread Id",
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ThreadStateResponse"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "Get Thread History"
      }
    },
    "/thread/{thread_id}/agents": {
      "get": {
        "description": "Retrieve node statuses and financial reports for the dashboard.",
        "operationId": "get_agent_statuses_thread__thread_id__agents_get",
        "parameters": [
          {
            "in": "path",
            "name": "thread_id",
            "required": true,
            "schema": {
              "title": "Thread Id",
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AgentStatusesResponse"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "Get Agent Statuses"
      }
    }
  }
}
